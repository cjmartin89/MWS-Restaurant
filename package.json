{
  "name": "mws-restaurant-stage-1-master",
  "version": "1.0.0",
  "description": "--- #### _Three Stage Course Material Project - Restaurant Reviews_",
  "main": "main.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/styles.css -w",
    "compile:sass": "node-sass css/style.comp.css",
    "concat:sass": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:sass prefix:css compress:css",
    "build": "gulp"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-clean-css": "^3.9.4",
    "gulp-image-resize": "^0.13.0",
    "gulp-uglify": "^3.0.1",
    "gulp-uglify-es": "^1.0.4",
    "gulp-watch": "^5.0.1",
    "node-sass": "^4.9.0",
    "pump": "^3.0.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "gulp-rename": "^1.2.3",
    "idb": "^2.1.3",
    "npm": "^6.1.0",
    "require": "^2.4.20"
  }
}
